{
  "name": "@ts-stack/type-is",
  "type": "module",
  "version": "1.0.1",
  "description": "A fork of the widely used `type-is` library, but this one accepts headers instead of a request object.",
  "exports": "./dist/index.js",
  "repository": "https://github.com/ts-stack/type-is",
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "test": "npm run build && npm run esm-jest",
    "esm-jest": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "build": "tsc -b tsconfig.build.json",
    "clean": "rimraf dist*"
  },
  "keywords": [
    "ts-stack",
    "mime"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "content-type": "^1.0.5",
    "mime-types": "^2.1.35"
  },
  "devDependencies": {
    "@types/content-type": "^1.1.8",
    "@types/eslint": "^8.56.11",
    "@types/jest": "^29.5.12",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.14.12",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "rimraf": "^5.0.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">= 20.6.0"
  }
}